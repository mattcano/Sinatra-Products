<div class="button_container"><a href="/" class="my_button">Home</a></div>

<table>
  <tr>
    <th>ID</th>
    <th>Name</th>
  </tr>
  <% @rs.each do |row| %>
    <tr>
      <td><%= row[0] %></td>
      <td><%= row[2] %></td>
      <td>
        <% if row[4] == "t"%>
        <img src="sale_icon.png" class="sale_icon">
        <% end %>
      </td>
    </tr>
  <% end %>
</table>